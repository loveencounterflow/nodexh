{
  "name": "nodexh",
  "version": "2.1.1",
  "description": "a wrapper around the `node` executable for better stacktraces with sourcemaps, sourcecode excerpts",
  "main": "lib/main.js",
  "bin": {
    "nodexh": "bin/nodexh"
  },
  "scripts": {
    "build": "coffee --map -o lib -c src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loveencounterflow/nodexh.git"
  },
  "keywords": [
    "node",
    "stacktrace",
    "terminal",
    "error handling"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loveencounterflow/nodexh/issues"
  },
  "homepage": "https://github.com/loveencounterflow/nodexh",
  "dependencies": {
    "error-callsites": "^2.0.3",
    "guy": "^12.3.0",
    "intertype": "^0.111.0",
    "load-source-map": "3.0.1",
    "stacktracey": "^2.0.14"
  }
}
